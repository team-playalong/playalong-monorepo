{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "jasmine": true,
    "jest": true
  },
  "globals": {
    "window": true,
    "app": true,
    "angular": false,
    "mockData": true,
    "inject": false,
    "dump": false
  },
  "extends": [
    "standard",
    "react-app",
    "plugin:react/recommended"
  ],
  "rules": {
    "indent": "off",
    "jsx-a11y/href-no-hash": [0],
    "no-tabs": "off",
    "no-var": "off",
    "space-before-function-paren": 0,
    "semi": 0,
    "strict": "off",
    "padded-blocks": "off",
    "no-param-reassign": "off",
    "consistent-return": "off",
    "func-names": "off",
    "object-shorthand": "off",
    "prefer-arrow-callback": "off",
    "prefer-template": "off",
    "no-else-return": "off",
    "no-use-before-define": "off",
    "no-mixed-spaces-and-tabs": "off",
    "brace-style": "off",
    "max-len": "off",
    "no-trailing-spaces": "off",
    "spaced-comment": "off",
    "wrap-iife": "off",
    "eol-last": "off",
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "comma-dangle": 0
  }
}
