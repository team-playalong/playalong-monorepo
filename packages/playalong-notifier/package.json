{
  "name": "playalong-notifier",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "serve": "node lib/index.js",
    "watch": "npm-run-all --parallel compile:watch serve:watch",
    "serve:watch": "nodemon lib/index.js",
    "compile:watch": "cross-env NODE_ENV=development babel src --out-dir lib --watch",
    "compile": "babel src --out-dir lib",
    "start": "npm-run-all compile serve",
    "copy:config": "cp -R config lib",
    "build": "cross-env NODE_ENV=production npm-run-all compile copy:config"
  },
  "dependencies": {
    "config": "^1.26.2",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "mailgun-js": "^0.13.1",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "webpack": "^3.8.1",
    "winston": "^2.4.0"
  }
}
