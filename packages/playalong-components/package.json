{
  "name": "playalong-components",
  "private": false,
  "scripts": {
    "start": "npm-run-all --parallel storybook test",
    "test": "jest --coverage",
    "test:watch": "node scripts/test.js --env=jsdom",
    "clean": "rimraf lib",
    "lint": "eslint --fix src/**/*.js",
    "prebuild": "npm-run-all clean",
    "build": "npm-run-all --parallel build:commonjs build:utils build:css build:copy-files",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib/components --ignore spec.js",
    "build:utils": "cross-env NODE_ENV=production babel ./src/utils --out-dir ./lib/utils --ignore spec.js",
    "deploy": "gh-pages -d build",
    "serve": "cd build && http-server",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o build"
  },
  "version": "0.1.19",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "author": {
    "name": "Dadi Atar",
    "email": "atardadi@gmail.com",
    "url": "https://www.playalong.io"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "components",
    "react",
    "react-component",
    "toolkit",
    "playalong"
  ],
  "dependencies": {
    "chokidar": "^1.7.0",
    "draft-js": "^0.10.5",
    "draft-js-export-html": "^1.2.0",
    "flag-icon-css": "^2.8.0",
    "font-awesome": "^4.7.0",
    "google-fonts-webpack-plugin": "^0.4.4",
    "hoek": "^5.0.3",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "lazysizes": "^4.0.0-rc3",
    "material-ui": "^0.20.1",
    "npm-run-all": "^4.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-fontawesome": "^1.6.1",
    "react-svg-gauge": "^1.0.4",
    "react-tooltip": "^3.3.0",
    "styled-components": "^2.0.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^3.4.7",
    "@storybook/addon-actions": "^3.4.7",
    "@storybook/addon-centered": "^3.4.7",
    "@storybook/addon-knobs": "^3.4.7",
    "@storybook/addon-storysource": "^3.4.8",
    "@storybook/react": "^3.4.7",
    "autoprefixer": "7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.1.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.1",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "fbjs": "^0.8.12",
    "file-loader": "0.11.2",
    "fs-extra": "4.0.0",
    "gh-pages": "^1.0.0",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "^22.4.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "^1.0.1",
    "webpack": "3.3.0",
    "webpack-dev-server": "2.5.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/enzyme.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "globals": {
      "window": true,
      "document": true
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
