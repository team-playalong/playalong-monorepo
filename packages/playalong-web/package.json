    "file-loader": "^1.1.6",
    "firebase-tools": "^3.16.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.6.1",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jshint-stylish": "^2.2.0",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "*",
    "ng-annotate-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.16",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "react-addons-test-utils": "^15.5.1",
    "react-dev-utils": "^4.2.1",
    "react-test-renderer": "^16.2.0",
    "react2angular": "^3.1.0",
    "sass-loader": "^6.0.3",
    "serve": "^6.4.1",
    "static": "^2.0.0",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "tslint": "^5.4.2",
    "typescript": "^2.6.2",
    "url-loader": "^0.6.2",
    "vinyl-fs": "3.0.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "Guitar-Tuner-Webapp": "https://github.com/oankarberg/Guitar-Tuner-Webapp.git",
