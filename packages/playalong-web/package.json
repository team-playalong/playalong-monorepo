    "angular-sanitize": "^1.6.7",
    "angular-touch": "^1.6.7",
    "angular-translate-interpolation-messageformat": "^2.16.0",
    "angular-translate-loader-static-files": "^2.16.0",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router": "~0.4.2",
    "angular-youtube-embed": "^1.1.1",
    "angularfire": "^2.3.0",
    "bootstrap": "^3.3.7",
    "es5-shim": "^4.5.10",
    "firebase": "^4.8.0",
    "flag-icon-css": "^2.9.0",
    "font-awesome": "^4.7.0",
